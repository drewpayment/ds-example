{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": false,
    "strict": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "es2019",
      "dom"
    ],
    "paths": {
      "@ajs/*": [
        "Scripts/ds/*"
      ],
      "@ds/core": [
        "lib/ds/core/src/lib"
      ],
      "@ds/core/*": [
        "lib/ds/core/src/lib/*"
      ],
      "@util/*": [
        "Scripts/util/*"
      ],
      "@ds/labor": [
        "lib/ds/labor/src/lib"
      ],
      "@ds/labor/*": [
        "lib/ds/labor/src/lib/*"
      ],
      "@ds/performance": [
        "lib/ds/performance/src/lib"
      ],
      "@ds/performance/*": [
        "lib/ds/performance/src/lib/*"
      ],
      "@ds/payroll": [
        "lib/ds/payroll/src/lib"
      ],
      "@ds/payroll/*": [
        "lib/ds/payroll/src/lib/*"
      ],
      "@ds/notifications": [
        "lib/ds/notifications/src/lib"
      ],
      "@ds/notifications/*": [
        "lib/ds/notifications/src/lib/*"
      ],
      "@ds/docs": [
        "apps/ds-docs/ui/src/app"
      ],
      "@ds/docs/*": [
        "apps/ds-docs/ui/src/app/*"
      ],
      "@scripts/ds": [
        "Scripts/ds"
      ],
      "@scripts/ds/*": [
        "Scripts/ds/*"
      ],
      "@ds/reports": [
        "lib/ds/reports/src/lib"
      ],
      "@ds/reports/*": [
        "lib/ds/reports/src/lib/*"
      ],
      "employee-services": [
        "apps/employee-services/src"
      ],
      "employee-services/*": [
        "apps/employee-services/src/*"
      ],
      "@ds/admin": [
        "lib/ds/admin/src/lib"
      ],
      "@ds/admin/*": [
        "lib/ds/admin/src/lib/*"
      ],
      "@ds/employee": [
        "lib/ds/employee/src/lib"
      ],
      "@ds/employee/*": [
        "lib/ds/employee/src/lib/*"
      ],
      "@ds/benefits": [
        "lib/ds/benefits/src/lib"
      ],
      "@ds/benefits/*": [
        "lib/ds/benefits/src/lib/*"
      ],
      "@ds/applicants": [
        "lib/ds/applicants/src/lib"
      ],
      "@ds/applicants/*": [
        "lib/ds/applicants/src/lib/*"
      ],
      "@ds/employees": [
        "lib/ds/employees/src/lib"
      ],
      "@ds/employees/*": [
        "lib/ds/employees/src/lib/*"
      ],
      "@ds/analytics": [
        "lib/ds/analytics/src/lib"
      ],
      "@ds/analytics/*": [
        "lib/ds/analytics/src/lib/*"
      ],
      "@ds/onboarding": [
        "lib/ds/onboarding/src/lib"
      ],
      "@ds/onboarding/*": [
        "lib/ds/onboarding/src/lib/*"
      ],
      "@models": [
        "lib/models/src/lib"
      ],
      "@models/*": [
        "lib/models/src/lib/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": false,
    "strictInputAccessModifiers": false,
    "strictTemplates": false
  },
  "exclude": [
    "node_modules",
    "test",
    "coverage",
    "prototree"
  ]
}
